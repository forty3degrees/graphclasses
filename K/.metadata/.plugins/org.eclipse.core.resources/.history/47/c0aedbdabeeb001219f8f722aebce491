package teo.isgci.core;

import teo.isgci.data.services.XmlDataProvider;
import teo.isgci.drawing.YFilesDrawingService;

public class App {
	public final static App Current = new App();
    
	public final IDataProvider DataProvider;
	public final IDrawingService DrawingService;
    public final ViewManager ViewManager;
	
	public App() {
		this.ViewManager = new ViewManager();
		this.DataProvider = new XmlDataProvider();
		this.DrawingService = new YFilesDrawingService();
		this.ViewManager.attachDrawingService(this.DrawingService);
	}
	
	
}
