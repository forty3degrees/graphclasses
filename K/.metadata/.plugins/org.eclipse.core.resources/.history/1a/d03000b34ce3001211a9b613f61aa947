package teo.data.services;

import teo.isgci.gc.GraphClass;

public interface IDataService {
	void loadData(String path) throws Exception;
	Relation findRelation(GraphClass x, GraphClass y);
	GraphClass[] getGraphClasses();
	Problem[] getProblems();
	void getSubClasses(GraphClass graph, Boolean recursive);
	void getSuperClasses(GraphClass graph, Boolean recursive);
	Map<Problem, ComplexityClass> getComplexityMap(GraphClass graph);
	Map<GraphClass, ComplexityClass> getComplexityMap(Problem problem);
}
